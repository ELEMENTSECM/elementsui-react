{
	"name": "elementsui-react",
	"version": "0.12.0",
	"main": "./lib/index.js",
	"typings": "./types/index.d.ts",
	"private": true,
	"engines": {
		"node": ">= 15.3.0"
	},
	"license": "MIT",
	"author": {
		"name": "Sikri AS",
		"email": "Asbjorn.Nordgaard@sikri.no",
		"url": "https://github.com/ELEMENTSECM/elementsui-react"
	},
	"homepage": "https://elementsecm.github.io/elementsui-react/",
	"files": [
		"lib"
	],
	"keywords": [
		"components",
		"react",
		"react-component",
		"elements"
	],
	"scripts": {
		"test": "node scripts/test.js",
		"deploy:docs": "gh-pages -d storybook-static",
		"clean": "rimraf dist",
		"build:lib": "npm run clean && npm run build:types && npm run build:es && npm run build:copy-files",
		"build:types": "tsc -p tsconfig.declaration.json",
		"build:es": "tsc -p tsconfig.es.json",
		"build:copy-files": "node scripts/copyBuildFiles.js",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"storybook": "start-storybook -s public",
		"storybook:build": "build-storybook -s public",
		"lint": "eslint src/**/*.{ts,tsx,js,jsx}"
	},
	"dependencies": {
		"classnames": "^2.3.1",
		"load-script": "^2.0.0",
		"lodash": "^4.17.21",
		"react-ada-datepicker": "^0.0.10",
		"react-bootstrap": "^0.33.1",
		"react-draggable-elements": "^4.4.2",
		"react-hotkeys": "^2.0.0",
		"react-overlays": "^4.1.1",
		"react-popper": "^1.3.4",
		"react-select": "^3.1.0",
		"react-spinners": "^0.11.0",
		"styled-components": "^5.3.3",
		"tslib": "^2.3.1"
	},
	"peerDependencies": {
		"react": "^17.0.0",
		"react-dom": "^17.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.16.12",
		"@babel/plugin-proposal-class-properties": "^7.16.7",
		"@babel/plugin-proposal-numeric-separator": "^7.16.7",
		"@babel/plugin-proposal-optional-chaining": "^7.16.7",
		"@babel/plugin-transform-modules-umd": "^7.16.7",
		"@babel/plugin-transform-runtime": "^7.16.10",
		"@babel/preset-env": "^7.16.10",
		"@babel/preset-react": "^7.16.7",
		"@babel/preset-typescript": "^7.16.7",
		"@sambego/storybook-state": "^2.0.1",
		"@storybook/addon-a11y": "^6.4.14",
		"@storybook/addon-console": "^1.2.3",
		"@storybook/addon-essentials": "^6.4.14",
		"@storybook/addon-knobs": "^6.4.0",
		"@storybook/addon-links": "^6.4.14",
		"@storybook/addon-storysource": "^6.4.14",
		"@storybook/builder-webpack5": "^6.4.14",
		"@storybook/manager-webpack5": "^6.4.14",
		"@storybook/react": "^6.4.14",
		"@types/enzyme": "^3.10.11",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^27.4.0",
		"@types/lodash": "^4.14.178",
		"@types/node": "15.3.0",
		"@types/react": "^17.0.38",
		"@types/react-bootstrap": "^0.32.22",
		"@types/react-dom": "^17.0.11",
		"@types/react-select": "^3.0.19",
		"@types/styled-components": "^5.1.21",
		"@typescript-eslint/eslint-plugin": "^5.10.0",
		"@typescript-eslint/parser": "^5.10.0",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^27.4.6",
		"babel-loader": "^8.2.3",
		"babel-plugin-inline-json-import": "^0.3.2",
		"babel-plugin-named-asset-import": "^0.3.8",
		"coveralls": "^3.1.1",
		"enzyme": "^3.11.0",
		"enzyme-to-json": "^3.6.2",
		"eslint": "^8.7.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"fs-extra": "^10.0.0",
		"gh-pages": "^3.2.3",
		"identity-obj-proxy": "3.0.0",
		"jest": "^27.4.7",
		"jest-environment-jsdom": "^27.4.6",
		"jest-watch-typeahead": "^1.0.0",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-app-polyfill": "^3.0.0",
		"react-dom": "^17.0.2",
		"react-is": "^17.0.2",
		"rimraf": "^3.0.2",
		"typescript": "^4.5.5",
		"webpack": "^5.67.0"
	},
	"jest": {
		"roots": [
			"<rootDir>/src"
		],
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/scripts/setupTests.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jest-environment-jsdom",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"modulePaths": [],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		],
		"watchPlugins": [
			"jest-watch-typeahead/filename",
			"jest-watch-typeahead/testname"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		]
	},
	"browserslist": [
		"defaults",
		"not IE 11"
	]
}
