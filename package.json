{
	"name": "elementsui-react",
	"version": "0.11.4",
	"main": "./lib/index.js",
	"typings": "./types/index.d.ts",
	"private": true,
	"engines": {
		"node": ">= 10.13.0"
	},
	"license": "MIT",
	"author": {
		"name": "Sikri AS",
		"email": "haakon.johannessen@sikri.no",
		"url": "https://github.com/ELEMENTSECM/elementsui-react"
	},
	"homepage": "https://elementsecm.github.io/elementsui-react/",
	"files": [
		"lib"
	],
	"keywords": [
		"components",
		"react",
		"react-component",
		"elements"
	],
	"scripts": {
		"test": "node scripts/test.js",
		"deploy:docs": "gh-pages -d storybook-static",
		"clean": "rimraf dist",
		"build:lib": "npm run clean && npm run build:types && npm run build:cjs && npm run build:copy-files",
		"build:types": "tsc -p tsconfig.declaration.json",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:copy-files": "node scripts/copyBuildFiles.js",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"storybook": "start-storybook -s public",
		"build-storybook": "build-storybook -s public"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"load-script": "^1.0.0",
		"lodash": "^4.17.20",
		"odatajs": "^4.0.0",
		"react-ada-datepicker": "^0.0.10",
		"react-bootstrap": "^0.33.1",
		"react-draggable-elements": "^4.4.2",
		"react-hotkeys": "^2.0.0",
		"react-overlays": "^4.1.1",
		"react-popper": "^1.3.4",
		"react-select": "^3.1.0",
		"react-spinners": "^0.9.0",
		"styled-components": "^5.1.1"
	},
	"peerDependencies": {
		"react": "^16.11.0",
		"react-dom": "^16.11.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.8",
		"@babel/core": "^7.12.9",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-numeric-separator": "^7.12.7",
		"@babel/plugin-proposal-optional-chaining": "^7.12.7",
		"@babel/plugin-transform-modules-umd": "^7.12.1",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-react": "^7.12.7",
		"@babel/preset-typescript": "^7.12.7",
		"@sambego/storybook-state": "^2.0.1",
		"@storybook/addon-a11y": "^6.1.6",
		"@storybook/addon-console": "^1.2.2",
		"@storybook/addon-essentials": "^6.1.6",
		"@storybook/addon-knobs": "^6.1.6",
		"@storybook/addon-links": "^6.1.6",
		"@storybook/addon-storysource": "^6.1.6",
		"@storybook/react": "^6.1.6",
		"@types/enzyme": "^3.10.5",
		"@types/fs-extra": "^9.0.1",
		"@types/jest": "^26.0.10",
		"@types/lodash": "^4.14.160",
		"@types/node": "12.11.7",
		"@types/react": "^16.9.47",
		"@types/react-bootstrap": "^0.32.22",
		"@types/react-dom": "^16.9.8",
		"@types/react-select": "^3.0.19",
		"@types/styled-components": "^5.1.2",
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.3.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-inline-json-import": "^0.3.2",
		"babel-plugin-named-asset-import": "^0.3.4",
