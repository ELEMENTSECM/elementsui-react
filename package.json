{
	"name": "elementsui-react",
	"version": "0.1.21",
	"main": "./lib/index.js",
	"private": true,
	"engines": {
		"node": ">=4.0.0"
	},
	"author": {
		"name": "EVRY AS",
		"email": "nicklas.pouey-winger@evry.com",
		"url": "https://github.com/EVRYAS/elementsui-react"
	},
	"homepage": "http://evryas.github.io/elementsui-react/",
	"files": ["lib"],
	"keywords": ["components", "react", "react-component", "elements"],
	"dependencies": {
		"commander": "^2.15.1",
		"inquirer": "^5.2.0",
		"lodash": "^4.17.5",
		"mkdirp": "^0.5.1",
		"moment": "^2.22.0",
		"office-ui-fabric-react": "^5.89.0",
		"promise": "8.0.1",
		"react": "^16.3.0",
		"react-dom": "^16.2.0",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"react-treeview": "^0.4.7"
	},
	"scripts": {
		"prestart": "npm run gen:docs",
		"start": "npm-run-all --parallel start:docs gen:docs-watch gen:types-watch",
		"start:docs": "node scripts/start.js",
		"gen:index": "node scripts/updateComponentIndex.js",
		"gen:types": "node scripts/generateTypings.js && prettier --config .prettierrc --write **/*.d.ts",
		"gen:types-watch": "npm run gen:types -- --watch",
		"gen:docs": "node scripts/generateComponentData.js",
		"gen:docs-watch": "npm run gen:docs -- --watch",
		"build:docs": "node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom --no-cache",
		"build:css": "cpx \"./src/components/**/*.css\" ./lib",
		"prebuild:lib": "rimraf lib",
		"build:lib":
			"npm-run-all --parallel build:commonjs build:css build:umd gen:types gen:index && npm run build:copy-files",
		"build:copy-package-files": "node scripts/copyBuildFiles.js",
		"build:typings": "cpx \"./src/components/**/*.d.ts\" ./lib",
		"build:index": "cpx \"./src/components/index.js\" ./lib",
		"build:copy-umd": "cpx \"./umd/*\" ./lib",
		"build:copy-files":
			"npm-run-all --parallel build:copy-package-files build:copy-umd build:typings build:index",
		"predeploy:docs": "npm run build:docs",
		"deploy:docs": "gh-pages -d build",
		"build:commonjs":
			"cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
		"build:umd":
			"cross-env NODE_ENV=production babel --plugins transform-es2015-modules-umd ./src/components --out-dir ./build/umd --ignore spec.js && cross-env NODE_ENV=production webpack --config config/webpack.config.umd.js && rimraf ./build/umd",
		"prepublish": "npm run build:lib",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,mjs}",
			"!src/**/index.js",
			"!src/**/index.d.ts",
			"!src/**/*.spec.js",
			"!src/docs/examples/**/*.js"
		],
		"setupFiles": ["<rootDir>/config/polyfills.js"],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": ["web.js", "mjs", "js", "json", "web.jsx", "jsx", "node"],
		"setupTestFrameworkScriptFile": "<rootDir>/scripts/testSetup.js"
	},
	"babel": {
		"presets": [
			[
				"env",
				{
					"modules": "commonjs"
				}
			],
			"react-app"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"devDependencies": {
		"autoprefixer": "7.1.6",
		"babel-cli": "^6.26.0",
		"babel-core": "6.26.0",
		"babel-eslint": "7.2.3",
		"babel-jest": "20.0.3",
		"babel-loader": "7.1.2",
		"babel-plugin-transform-es2015-modules-umd": "^6.24.1",
		"babel-preset-react-app": "^3.1.1",
		"babel-runtime": "6.26.0",
		"case-sensitive-paths-webpack-plugin": "2.1.1",
		"chalk": "1.1.3",
		"chokidar": "^2.0.2",
		"coveralls": "^3.0.0",
		"cpx": "^1.5.0",
		"cross-env": "^5.1.3",
		"css-loader": "0.28.7",
		"dotenv": "4.0.0",
		"dotenv-expand": "4.2.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "4.10.0",
		"eslint-config-react-app": "^2.1.0",
		"eslint-loader": "1.9.0",
		"eslint-plugin-flowtype": "2.39.1",
		"eslint-plugin-import": "2.8.0",
		"eslint-plugin-jsx-a11y": "5.1.1",
		"eslint-plugin-react": "7.4.0",
		"file-loader": "1.1.5",
		"fs-extra": "3.0.1",
		"gh-pages": "^1.1.0",
		"highlight.js": "^9.12.0",
		"html-webpack-plugin": "^3.2.0",
		"jest": "22.4.3",
		"npm-run-all": "^4.1.2",
		"object-assign": "4.1.1",
		"postcss-flexbugs-fixes": "3.2.0",
		"postcss-loader": "2.0.8",
		"prettier": "^1.12.1",
		"promise": "8.0.1",
		"raf": "3.4.0",
		"react-addons-test-utils": "^15.6.2",
		"react-dev-utils": "^5.0.1",
		"react-docgen": "^2.20.1",
		"react-test-renderer": "^16.2.0",
		"react-to-typescript-definitions": "^0.28.1",
		"rimraf": "^2.6.2",
		"style-loader": "0.19.0",
		"sw-precache-webpack-plugin": "0.11.4",
		"url-loader": "0.6.2",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.1.3",
		"webpack-dev-server": "2.9.4",
		"webpack-manifest-plugin": "^2.0.2",
		"whatwg-fetch": "2.0.3",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"mini-css-extract-plugin": "^0.4.0",
		"webpack-node-externals": "^1.7.2"
	},
	"typings": "./lib/index.d.ts",
	"bin": "./bin/index.js"
}
