{
	"name": "elementsui-react",
	"version": "0.2.1",
	"private": true,
	"author": {
		"name": "EVRY AS",
		"email": "nicklas.pouey-winger@evry.com",
		"url": "https://github.com/EVRYAS/elementsui-react"
	},
	"homepage": "http://evryas.github.io/elementsui-react/",
	"files": [
		"lib"
	],
	"keywords": [
		"components",
		"react",
		"react-component",
		"elements"
	],
	"scripts": {
		"start": "npx styleguidist server",
		"build:docs": "npx styleguidist build",
		"build:es6": "tsc -p tsconfig.es6.json",
		"build:commonjs": "tsc -p tsconfig.commonjs.json",
		"build:umd": "tsc -p tsconfig.umd.json && cross-env NODE_ENV=production webpack --config config/webpack.config.umd.js",
		"build:umd:min": "tsc -p tsconfig.umd.json && cross-env NODE_ENV=production webpack --config config/webpack.config.umd.min.js",
		"build": "npm run gen:index && npm run gen:types && npm-run-all --parallel build:es6 build:commonjs build:umd build:umd:min copy:types copy:package-files",
		"publish:lib": "npm version patch && npm run build && cd lib && npm publish",
		"publish:lib:alpha": "npm version patch && npm run build && cd lib && npm publish --tag alpha",
		"test": "node scripts/test.js --env=jsdom",
		"gen:index": "node scripts/updateComponentIndex.js",
		"gen:types": "node scripts/generateTypings.js && prettier --config .prettierrc --write **/*.d.ts",
		"copy:types": "cpx src/components/**/*.d.ts lib",
		"copy:package-files": "node scripts/copyBuildFiles.js"
	},
	"dependencies": {
		"@types/react-bootstrap": "^0.32.14",
		"emotion": "^9.2.9",
		"moment": "^2.22.2",
		"object-assign": "4.1.1",
		"react": "^16.5.1",
		"react-bootstrap": "^0.32.4",
		"react-datetime-contrib": "^1.0.0",
		"react-dev-utils": "^5.0.1",
		"react-dom": "^16.5.1",
		"react-emotion": "^9.2.9",
		"react-intl": "^2.6.0",
		"inquirer": "^5.2.0",
		"commander": "^2.15.1"
	},
	"devDependencies": {
		"@types/enzyme": "^3.1.14",
		"@types/jest": "^23.3.2",
		"@types/node": "^10.10.1",
		"@types/react": "^16.4.14",
		"@types/react-dom": "^16.0.7",
		"cpx": "^1.5.0",
		"cross-env": "^5.2.0",
		"enzyme": "^3.6.0",
		"enzyme-adapter-react-16": "^1.5.0",
		"npm-run-all": "^4.1.3",
		"prettier": "^1.14.2",
		"react-docgen-typescript": "^1.8.1",
		"react-styleguidist": "^7.3.8",
		"typescript": "^3.0.3",
		"typings-for-css-modules-loader": "^1.7.0",
		"autoprefixer": "7.1.6",
		"babel-jest": "^22.1.0",
		"babel-loader": "^7.1.2",
		"babel-preset-react-app": "^3.1.1",
		"case-sensitive-paths-webpack-plugin": "2.1.1",
		"chalk": "1.1.3",
		"css-loader": "0.28.7",
		"dotenv": "4.0.0",
		"dotenv-expand": "4.2.0",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "0.11.2",
		"fork-ts-checker-webpack-plugin": "^0.2.8",
		"fs-extra": "3.0.1",
		"html-webpack-plugin": "2.29.0",
		"jest": "22.4.2",
		"postcss-flexbugs-fixes": "3.2.0",
		"postcss-loader": "2.0.8",
		"raf": "3.4.0",
		"resolve": "1.6.0",
		"source-map-loader": "^0.2.1",
		"style-loader": "0.19.0",
		"sw-precache-webpack-plugin": "0.11.4",
		"ts-jest": "22.0.1",
		"ts-loader": "^2.3.7",
		"tsconfig-paths-webpack-plugin": "^2.0.0",
		"tslint": "^5.7.0",
		"tslint-config-prettier": "^1.10.0",
		"tslint-react": "^3.2.0",
		"uglifyjs-webpack-plugin": "^1.1.8",
		"url-loader": "0.6.2",
		"webpack": "3.8.1",
		"webpack-dev-server": "2.9.4",
		"webpack-manifest-plugin": "1.3.2",
		"whatwg-fetch": "2.0.3"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
			"<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"web.js",
			"js",
			"web.jsx",
			"jsx",
			"json",
			"node",
			"mjs"
		],
		"globals": {
			"ts-jest": {
				"tsConfigFile": "/Users/deniskashkovsky/projects/elementsui-typescript/elementsui-react/tsconfig.test.json"
			}
		},
		"setupTestFrameworkScriptFile": "<rootDir>/scripts/testSetup.js"
	},
	"babel": {
		"presets": [
			"react-app"
		]
	},
	"bin": "./bin/index.js"
}
